<% form_for(@what_new, :url => {:controller => 'what_news', :action => 'update'}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title, 'Tytuł' %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content, 'Treść' %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Zapisz' %>
  </p>
<% end %>

<%= link_to 'Back', what_news_index_path %>
