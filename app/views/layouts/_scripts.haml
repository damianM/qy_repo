= form_authenticity_token_tag

= javascript_include_tag 'swfupload/swfupload.js', 'swfupload/fileprogress.js'

= javascript_include_tag 'jquery/jquery-1.3.2', 'jquery/jquery.livequery', 'jquery/jquery.inherit-1.0.9', 'jquery/jquery.form', 'jquery/jquery.delegate', 'jquery/jquery-validate/jquery.validate', 'jquery/jquery.log', 'jquery/jquery.string', 'jquery/jquery.utils', 'jquery/jquery.ons.utils', 'jquery/ajaxupload', 'jquery/jquery.timers', 'jquery/jquery.rating'

= javascript_include_tag 'flashobject'
  
= javascript_include_tag 'application_jquery', 'admin', 'flash_message', 'ajax_upload_handler', 'google_maps', 'ad', 'video', 'photo'
= javascript_include_tag 'swfupload_handlers', 'swfuploadControl' if params[:controller] == 'galleries' and params[:action] == 'show'

= javascript_include_tag "flowplayer-3.1.1.min.js"


= stylesheet_link_tag "main", 'swfupload'
= stylesheet_link_tag 'jquery.rating'

