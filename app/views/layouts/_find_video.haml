      = text_field_tag :search, params[:search]

      %div
        = link_to "Szczegółowe wyszukiwanie", {:controller => "video", :action => "dfind"}, :style => "font-weight: normal; font-size: 8pt"

      .form_row
        = submit_tag "Zapisz", :class => 'submit', :style => "margin-top:10px;"

    .clr