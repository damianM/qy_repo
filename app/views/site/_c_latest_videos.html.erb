<div class="partial latest_items">
  <%=insert_corners%>
  <div class="header">
    Ostatnio dodane filmy Twoich znajomych
  </div>
  <div class='body'>
     <% if videos.length > 0%>
    <% for video in videos  %>
      <div class="latest_item">
        <div>
          <%= link_ image_tag(video.link[0...-3] + "jpg"), "video", "show", video.id %><br/>
        </div>
        <%= link_ video.gallery.user.login, "user", "show", video.gallery.user.id%>
      </div>

    <% end %>
     <% else %>
      Nikt z Twoich znajomych nie doda≈Ç jeszcze filmu
    <% end %>
    <div style="clear:both">
    </div>
  </div>
</div>