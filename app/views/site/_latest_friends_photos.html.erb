<div class="partial latest_items">
  <div class="header p_latest_friends_photos"></div>
  <div class='body'>

    <% if photos.length > 0%>

      <%  for photo in photos  %>
        <div class="latest_item">
          <div>
            <%= link_to image_tag(photo.public_filename(:thumb)), photo_path(photo) %><br/>
          </div>
          <%= link_to photo.gallery.galleriable.login, user_path(photo.gallery.galleriable) %>
        </div>

      <% end %>
    <% else %>
      Nikt z Twoich znajomych nie dodał jeszcze zdjęcia
    <% end %>
    <div style="clear:both">
    </div>
  </div>
</div>
