<div class="partial">
  
  <div class="header p_team_myteams"></div>
  <div class="body">
    <% unless teams.empty? %>
      <% for team in teams %>
        <%= render :partial => "team/team", :locals => {:team => team} %>

      <% end %>
    <% else %>
      Nie należysz do żadnej grupy
    <% end %>
    <div class="button_gold_100">
      <%= link_ "Dodaj grupę" , "team", "new" %>
    </div>
  </div>
</div>